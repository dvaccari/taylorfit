{
  "name": "taylorfit",
  "version": "0.0.0",
  "description": "taylorfit",
  "main": "index.js",
  "scripts": {
    "build": "npm install && npm run build:engine && webpack",
    "build:engine": "browserify engine/index.es6 -o build/engine.js -s engine --noparse node_modules/mathjs/dist/math.min.js -t [ babelify --presets [ es2015 ] ]",
    "build:worker": "browserify worker/worker.es6 -o worker/bundle.js -s worker -t [ babelify --presets [ es2015 ] ]",
    "debug": "npm install && webpack-dev-server --inline --hot",
    "test": "mocha --recursive test"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.11.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "mocha": "^3.1.2",
    "nib": "^1.1.2",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "fast-csv": "^2.3.0",
    "knockout": "^3.4.0",
    "mathjs": "^3.6.0",
    "papaparse": "^4.1.2"
  }
}
